{
  "Recipe": {
    "title": {
      "loremSentences": {
        "minLength": 1,
        "maxLength": 3
      }
    },
    "recipePictureUrl": {
      "regexp": "https://spoonacular.com/recipeImages/\\[0-9]\\{5}-556x370.jpg"
    },
    "steps": {  "hasMany": "Step" },
    "ingredients": { "hasMany": "Ingredient" }

  },

  "Step": {
    "body": {
      "loremSentences": {
        "minLength": 5,
        "maxLength": 20
      }
    }
  },

  "Ingredient": {
    "name": {
      "regexp": "[A-Z][a-z]{5,10}"
    },
    "amount": { "regexp": "[0-9]{1,5)"},
    "units": {"string":  ["KG", "L", "ML", "G", "UNITS"]}
  }

}



