{
  "Recipe": {
    "title": {
      "loremWords": {
        "minLength": 2,
        "maxLength": 20
      }
    },
    "recipePictureUrl": {
      "regexp": "https://spoonacular\\.com/recipeImages/[0-9]{5}-556x370.jpg"
    },
    "howToMake": {
      "loremParagraphs": {
        "minLength": 300,
        "maxLength": 800
      }
    },
    "ingredients": {
      "hasMany": "Ingredient"
    }
  },

  "Ingredient": {
    "productName": {
      "regexp": "[A-Z][a-z]{5,10}"
    },
    "amount": {
      "regexp": "[0-9]{1,5)"
    },
    "units": {
      "string": [
        "KG",
        "L",
        "ML",
        "G",
        "UNITS"
      ]
    }
  }
}



